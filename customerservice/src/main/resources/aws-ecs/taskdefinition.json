{
  "containerDefinitions":
  [
    {
      "environment" : [
        {
          "name": "SERVICE_9997_NAME",
          "value": "customer"
        },
        {
          "name": "SERVICE_9997_CHECK_HTTP",
          "value": "/customer"
        },
        {
          "name": "SERVICE_9997_CHECK_INTERVAL",
          "value": "10s"
        },
        {
          "name": "SERVICE_TAGS",
          "value": "http"
        }
      ],
      "name" : "customerservice",
      "image" : "<your_dockerhub_name>/customerservice:3",
      "memory":200,
      "portMappings":[
        {
          "containerPort" : 9997,
          "hostPort" : 8090
        }
      ],
      "essential":true
    }
  ],
  "family":"<TeamName>-CustomerServiceTask"
}